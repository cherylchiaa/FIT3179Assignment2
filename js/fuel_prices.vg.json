{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "Boxplot for fuel prices over time",
    "title": "Fuel Prices by Year and Quarter",
    "width": 600,
    "height": 400,
    "data": {
      "url": "https://raw.githubusercontent.com/cherylchiaa/FIT3179Assignment2/refs/heads/main/data/AustralianFuelPrices.csv",
      "format": {
        "type": "csv"
      }
    },
    "transform": [
      {
        "fold": [
          "Premium unleaded petrol (95 RON) (cpl)",
          "Regular unleaded petrol (91 RON) (cpl)",
          "Automotive diesel (cpl)",
          "Automotive LPG (cpl)"
        ],
        "as": ["Fuel Type", "Price (cpl)"]
      }
    ],
    "mark": "boxplot",
    "encoding": {
      "x": {"field": "Year", "type": "ordinal", "title": "Year"},
      "y": {"field": "Price (cpl)", "type": "quantitative", "title": "Price (cpl)"},
      "color": {"field": "Fuel Type", "type": "nominal", "title": "Fuel Type"}
    }
  }
  